Console.WriteLine("Введите число A: ");
int A = int.Parse(Console.ReadLine());
Console.WriteLine("Введите число B: ");
int B = int.Parse(Console.ReadLine());
int count = B;
int result = 1;

for (int i = 0; i < count; i++)
{

    result *= A;
}
Console.WriteLine($"Число {A} в степени {B} -> {result} ");
